{
  "name": "react-mwc",
  "version": "0.0.1",
  "author": "James Friedman",
  "private": false,
  "description": "A thin React wrapper for Material Design (Web) Components",
  "homepage": "https://jamesmfriedman.github.io/rmwc/",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesmfriedman/rmwc.git"
  },
  "keywords": [
    "rmwc",
    "react",
    "react-material",
    "material-design",
    "react-mdc",
    "react-material-design"
  ],
  "bugs": {
    "url": "https://github.com/jamesmfriedman/rmwc/issues",
    "email": "me@jamesmfriedman.com"
  },
  "license": "MIT",
  "main": "index.js",
  "dependencies": {
    "classnames": "^2.2.x",
    "material-components-web": "^0.24.0",
    "prop-types": "^15.6.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "babelfy": "node scripts/babelfy.js",
    "build": "node scripts/build.js",
    "build:lib": "node scripts/build-lib.js",
    "deploy:docs": "npm run build:lib && npm run build && git add build -f && git commit -m 'running build' && git subtree split -b gh-pages --prefix build && git push origin gh-pages:gh-pages --force && git branch -D gh-pages",
    "changelog": "changelog",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "preversion": "npm test -- --coverage",
    "version": "npm run build:lib && git add -A lib && npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "fix": "prettier --write \"src/**/*.js\""
  },
  "prettier": {
    "semi": true,
    "printWidth": 120,
    "singleQuote": true,
    "jsxBracketSameLine": false
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/**/*.{js,jsx}",
      "!<rootDir>/**/*.story.js",
      "!<rootDir>/Base/story-with-state.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/scripts/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/**/?(*.)(spec).js?(x)"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "react-hot-loader/babel",
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      [
        "transform-class-properties",
        {
          "spec": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs",
            {
              "loose": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-replace-object-assign": "^0.2.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "css-to-string-loader": "^0.1.3",
    "dotenv": "4.0.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "generate-changelog": "^1.5.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "normalize.css": "^7.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "prettier": "^1.8.2",
    "promise": "8.0.1",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^4.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^5.7.0",
    "react-test-renderer": "^16.0.0",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  }
}
